# Shared
NODE_ENV=development

# API
PORT=3001
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/jamming_fun
ENABLE_MAGICBLOCK=true
ENABLE_AUDIOUS=true
ENABLE_BLINKS=true
SOLANA_CLUSTER=devnet
SOLANA_RPC_URL=https://api.devnet.solana.com
AUDIOUS_APP_NAME=jamming.fun
AUDIOUS_WRITE_MODE=read_only
# Optional Audius real credentials (server-side only)
# AUDIOUS_API_KEY=
# AUDIOUS_API_SECRET=
# AUDIOUS_BEARER_TOKEN=

# Optional MagicBlock SOAR (server-side only)
# MAGICBLOCK_SOLANA_RPC_URL=https://api.devnet.solana.com
# MAGICBLOCK_AUTH_WALLET_PRIVATE_KEY=
# MAGICBLOCK_SOAR_GAME_PUBKEY=
# MAGICBLOCK_SOAR_LEADERBOARD_PUBKEY=
# MAGICBLOCK_SOAR_ACHIEVEMENT_PUBKEY=

# Optional Blinks override RPC
# BLINKS_SOLANA_RPC_URL=https://api.devnet.solana.com

# Web (Vite)
VITE_API_URL=http://localhost:3001/api/v1
VITE_WS_URL=ws://localhost:3001/ws
VITE_SOLANA_CLUSTER=devnet
VITE_SAMPLE_KIT_BASE_URL=/samples/hydrogen-lite
