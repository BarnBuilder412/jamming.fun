    "@jamming/pattern-core": "workspace:*",
    "@jamming/shared-types": "workspace:*",
    "@prisma/client": "^6.3.1",
    "fastify": "^5.2.1",
    "pino-pretty": "^13.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@jamming/config-eslint": "workspace:*",
    "@jamming/config-ts": "workspace:*",
    "prisma": "^6.3.1"
